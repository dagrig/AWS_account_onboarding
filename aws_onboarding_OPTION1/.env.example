# .env
TF_VAR_aws_region="us-west-2"
TF_VAR_account_name="my-aws-account"
TF_VAR_account_email="my-email@example.com"
TF_VAR_ou_name="my-ou"
TF_VAR_domain="example.com"

# DNS records in JSON format
TF_VAR_dns_records='[
  {
    "name": "www",
    "type": "A",
    "ttl": 300,
    "records": ["1.2.3.4"]
  }
]'

TF_VAR_identity_store_id="your_identity_store_id"
TF_VAR_group_name="AdminGroup"
TF_VAR_permission_set_name="AdminPermissionSet"